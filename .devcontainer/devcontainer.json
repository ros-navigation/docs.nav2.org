{
    "name": "Nav2 Docs",
    "build": {
        "dockerfile": "../Dockerfile",
        "context": ".."
    },
    "workspaceMount": "source=${localWorkspaceFolder},target=/docs,type=bind",
    "workspaceFolder": "/docs",
    "customizations": {
        "vscode": {
            "settings": {},
            "extensions": [
                "eamodio.gitlens",
                "esbenp.prettier-vscode",
                "GitHub.copilot",
                "streetsidesoftware.code-spell-checker"
            ]
        }
    }
}
